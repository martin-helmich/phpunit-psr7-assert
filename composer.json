{
    "name": "helmich/phpunit-psr7-assert",
    "description": "PHPUnit assertions for testing PSR7-compliant applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Helmich",
            "email": "m.helmich@mittwald.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "helmich/phpunit-json-assert": "^3.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.2",
        "phpspec/prophecy": "^1.17",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0"
    },
    "conflict": {
        "phpunit/phpunit": "<8.0 || >= 11.0"
    },
    "autoload": {
        "psr-4": {
            "Helmich\\Psr7Assert\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/helmich/phpunit-json-assert/src/Functions.php",
            "src/Functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
